from re import S
from readline import insert_text


class Node:
    def __init__(self, data=None, next=None) -> None:
        self.data = data
        self.next = next


class LinkedList:
    def __init__(self):
        self.head = None

    def insert_at_beginning(self, data):
        node = Node(data, self.head)
        self.head = node

    def insert_at_end(self, data):
        node = Node(data, None)
        root = self.head
        while root.next:
            root = root.next
        root.next = node

    def insert(self, data):
        node = Node(data, None)
        if not self.head:
            self.head = node
            return
        root = self.head
        while root.next:
            root = root.next
        root.next = node

    def print(self):
        root = self.head
        s = ''
        while root:
            s += str(root.data) + '-->'
            root = root.next
        print(s[:len(s)-3])


if __name__ == '__main__':
    l = LinkedList()
    l.insert_at_beginning(5)
    l.insert_at_beginning(4)
    l.insert_at_end(7)
    l.insert_at_beginning(3)
    l.insert_at_end(10)
    l2 = LinkedList()
    l2.insert(1)
    l2.insert(2)
    l2.insert(3)
    l2.print()
